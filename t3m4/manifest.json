{
    "manifest_version": 2,
    "name": "T3-M4",
    "version": "1.0",
    "description": "Utility Droid",
    "permissions": ["tabs", "activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "assets/t3m4.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "64": "assets/t3m4.png"
    }
  }
  